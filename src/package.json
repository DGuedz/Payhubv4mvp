{
  "name": "payhub-v3",
  "version": "1.0.0",
  "description": "PAYHUB - Tesouraria ativa para PMEs com liquidez D+0 em RLUSD",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "qa:audit": "node scripts/qa-audit.js",
    "test:endpoints": "node scripts/endpoint-test-runner.js",
    "setup:testnet": "bash scripts/setup-testnet-envs.sh",
    "test:e2e": "bash scripts/run-e2e-testnet.sh"
  },
  "keywords": [
    "xrpl",
    "rlusd",
    "escrow",
    "pix",
    "treasury",
    "defi",
    "compliance"
  ],
  "author": "PAYHUB Team",
  "license": "MIT",
  "dependencies": {
    "lucide-react": "^0.553.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
